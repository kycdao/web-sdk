{
	"name": "@kycdao/kycdao-web-sdk",
	"version": "0.3.0-rc.37",
	"license": "unlicensed",
	"main": "dist/umd/index.js",
	"types": "./dist/types/index.d.ts",
	"scripts": {
		"install-precommit-hook": "husky install .husky",
		"start": "yarn start:sass & yarn start-https",
		"start-https": "cross-env HTTPS=true PORT=4002 GENERATE_SOURCEMAP=false DISABLE_ESLINT_PLUGIN=true BROWSER=none react-app-rewired start",
		"start-http": "cross-env HTTPS=false PORT=4002 GENERATE_SOURCEMAP=false DISABLE_ESLINT_PLUGIN=true BROWSER=none react-app-rewired start",
		"build:lib": "tsc --project tsconfig.esm.json & tsc --project tsconfig.umd.json && tsc-alias -p tsconfig.umd.json & tsc-alias -p tsconfig.esm.json",
		"build:web": "GENERATE_SOURCEMAP=false react-app-rewired build",
		"build": "yarn clean && yarn build:sass & yarn build:lib & yarn build:web && mv ./build/* ./dist/ && rm ./dist/index.html & rm ./dist/iframe.html",
		"test": "react-app-rewired test",
		"eject": "create-react-app --eject",
		"lint": "eslint 'src/**/*.{ts,tsx,scss}' --fix",
		"format": "prettier -w package.json .github/ src/",
		"clean": "rimraf build dist",
		"build:sass": "sass ./src/style/index.scss dist/app.css --style=compressed & sass ./src/KycDaoClient.scss --style=compressed dist/client.css & sass --style=compressed ./src/KycDaoIframeClient.scss dist/iframeClient.css",
		"start:sass": "sass --watch ./src/style/index.scss public/app.css --style=compressed & sass --watch ./src/KycDaoClient.scss --style=compressed public/client.css & sass --watch --style=compressed ./src/KycDaoIframeClient.scss public/iframeClient.css"
	},
	"files": [
		"dist/*",
		"README.md",
		"index.d.ts"
	],
	"repository": {
		"url": "https://github.com/kycdao/web-sdk",
		"type": "git"
	},
	"exports": {
		".": {
			"import": "./dist/esm/index.js",
			"require": "./dist/umd/index.js",
			"style": "./dist/app.css"
		},
		"./app.css": {
			"import": "./dist/app.css"
		},
		"./client.css": {
			"import": "./dist/client.css"
		},
		"./iframeClient.css": {
			"import": "./dist/iframeClient.css"
		},
		"./client": {
			"import": "./dist/esm/client.js",
			"script": "./dist/client.min.js",
			"style": "./dist/client.css",
			"require": "./dist/umd/client.js"
		},
		"./iframeClient": {
			"import": "./dist/esm/iframeClient.js",
			"script": "./dist/iframeClient.min.js",
			"style": "./dist/iframeClient.css",
			"require": "./dist/umd/client.js"
		},
		"./esm": "./dist/esm/index.js",
		"./umd": "./dist/umd/index.js",
		"./esm/**/*.*": "./dist/esm/**/*.*",
		"./umd/**/*.*": "./dist/umd/**/*.*",
		"./dist/**/*.*": "./dist/**/*.*"
	},
	"dependencies": {
		"@kycdao/kycdao-sdk": "^0.4.1",
		"buffer": "^6.0.3",
		"clsx": "^1.2.1",
		"near-api-js": "0.44.2",
		"persona": "^4.7.1",
		"react-error-boundary": "^3.1.4",
		"react-scripts": "^5.0.1",
		"react-swipeable": "^7.0.0",
		"rxjs": "^7.8.0",
		"stream": "^0.0.2",
		"styled-components": "^5.3.6"
	},
	"peerDependencies": {
		"react": "^18",
		"react-dom": "^18"
	},
	"devDependencies": {
		"@babel/plugin-transform-typescript": "^7.20.7",
		"@babel/preset-env": "^7.20.2",
		"@babel/preset-react": "^7.18.6",
		"@playwright/test": "^1.29.1",
		"@testing-library/jest-dom": "^5.16.5",
		"@testing-library/react": "^13.4.0",
		"@testing-library/user-event": "^14.4.3",
		"@types/jest": "^29.2.5",
		"@types/node": "^18.11.18",
		"@types/react": "^18.0.26",
		"@types/react-dom": "^18.0.10",
		"@types/styled-components": "^5.1.26",
		"@typescript-eslint/eslint-plugin": "^5.48.0",
		"@typescript-eslint/parser": "^5.48.0",
		"babel-cli": "^6.26.0",
		"babel-preset-es2015": "^6.24.1",
		"create-react-app": "^5.0.1",
		"cross-env": "^7.0.3",
		"eslint": "^8.31.0",
		"eslint-config-airbnb": "^19.0.4",
		"eslint-config-prettier": "^8.6.0",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-jsx-a11y": "^6.6.1",
		"eslint-plugin-playwright": "^0.11.2",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-react": "^7.31.11",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-rxjs": "^5.0.2",
		"husky": "^8.0.3",
		"lint-staged": "^13.1.0",
		"npm-dts-webpack-plugin": "^1.3.12",
		"prettier": "^2.8.1",
		"react": "^18.2.0",
		"react-app-rewired": "^2.2.1",
		"react-dom": "^18.2.0",
		"rimraf": "^3.0.2",
		"sass": "^1.57.1",
		"tsc": "^2.0.4",
		"tsc-alias": "^1.8.2",
		"typescript": "^4.9.4"
	},
	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest"
		]
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"packageManager": "yarn@4.0.0-rc.33",
	"lint-staged": {
		"{*.ts,*.tsx,*.scss}": [
			"prettier --write",
			"eslint"
		],
		"*.html": "prettier --write"
	}
}
