  "scripts": {
    "start": "cross-env HTTPS=true GENERATE_SOURCEMAP=false DISABLE_ESLINT_PLUGIN=true BROWSER=none react-app-rewired start",
    "build:lib": "yarn clean && GENERATE_SOURCEMAP=false react-app-rewired build && mv ./build ./dist && mv index.d.ts dist/",
    "build:client": "tsc --outDir dist --esModuleInterop --module commonjs --target es6 ./src/KycDaoClient.ts --declaration --declarationMap --sourcemap",
    "test": "react-app-rewired test",
    "eject": "create-react-app --eject",
    "lint": "eslint --ext ts,tsx src/*/*.*",
    "clean": "rimraf build dist"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "packageManager": "yarn@4.0.0-rc.26"
