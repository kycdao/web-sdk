{
	"name": "@kycdao/kycdao-web-sdk",
	"version": "0.3.0-rc.7",
	"license": "unlicensed",
	"scripts": {
		"postinstall": "husky install .husky",
		"start": "cross-env HTTPS=true PORT=4002 GENERATE_SOURCEMAP=false DISABLE_ESLINT_PLUGIN=true BROWSER=none react-app-rewired start",
		"build:lib": "tsc",
		"build:web": "GENERATE_SOURCEMAP=false react-app-rewired build",
		"build": "yarn clean && yarn build:lib && yarn build:web && mv ./build/* ./dist/ && yarn run copy-files",
		"test": "react-app-rewired test",
		"eject": "create-react-app --eject",
		"lint": "eslint 'src/**/*.{ts,tsx,scss}' --fix",
		"format": "prettier -w src/",
		"clean": "rimraf build dist",
		"copy-files": "copyfiles -u 1 \"./src/**/*.css\" \"./src/**/*.scss\" ./dist/es "
	},
	"files": [
		"dist/*",
		"README.md"
	],
	"repository": {
		"url": "https://github.com/kycdao/web-sdk",
		"type": "git"
	},
	"exports": {
		".": {
			"import": "./dist/es/index.js",
			"style": "./dist/app.css"
		},
		"./KycDaoClient": {
			"import": "./dist/KycDaoClient.js",
			"script": "./dist/client.min.js",
			"style": "./dist/client.css"
		},
		"./IframeClient": {
			"import": "./dist/IframeClient.js",
			"script": "./dist/iframeClient.min.js",
			"style": "./dist/iframeClient.css"
		},
		"./es": "./dist/es/"
	},
	"types": "./index.d.ts",
	"dependencies": {
		"@kycdao/kycdao-sdk": "^0.3.12",
		"@testing-library/jest-dom": "^5.16.5",
		"@testing-library/react": "^13.4.0",
		"@testing-library/user-event": "^14.4.3",
		"@types/jest": "^29.2.3",
		"@types/node": "^18.11.9",
		"@types/react": "^18.0.25",
		"@types/react-dom": "^18.0.9",
		"buffer": "^6.0.3",
		"crypto-browserify": "^3.12.0",
		"near-api-js": "^1.1.0",
		"persona": "^4.7.1",
		"process": "^0.11.10",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-error-boundary": "^3.1.4",
		"react-scripts": "^5.0.1",
		"react-swipeable": "^7.0.0",
		"rxjs": "^7.5.7",
		"sass": "^1.56.1",
		"stream": "0.0.2",
		"styled-components": "^5.3.6"
	},
	"devDependencies": {
		"@babel/plugin-transform-typescript": "^7.20.2",
		"@babel/preset-env": "^7.20.2",
		"@babel/preset-react": "^7.18.6",
		"@typescript-eslint/eslint-plugin": "^5.43.0",
		"@typescript-eslint/parser": "^5.43.0",
		"babel-cli": "^6.26.0",
		"babel-preset-es2015": "6.24.1",
		"copy-files": "^0.1.0",
		"copyfiles": "^2.4.1",
		"create-react-app": "^5.0.1",
		"cross-env": "^7.0.3",
		"eslint": "^8.28.0",
		"eslint-config-airbnb": "^19.0.4",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-jsx-a11y": "^6.6.1",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-react": "^7.31.11",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-rxjs": "^5.0.2",
		"husky": "^8.0.2",
		"lint-staged": "^13.1.0",
		"npm-dts-webpack-plugin": "^1.3.12",
		"prettier": "^2.7.1",
		"react-app-rewired": "^2.2.1",
		"rimraf": "^3.0.2",
		"tsc": "^2.0.4",
		"typescript": "4.9.3"
	},
	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest"
		]
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"packageManager": "yarn@4.0.0-rc.32",
	"lint-staged": {
		"{*.ts,*.tsx,*.scss}": [
			"prettier --write",
			"eslint"
		],
		"*.html": "prettier --write"
	}
}
